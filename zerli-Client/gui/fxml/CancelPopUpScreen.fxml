      </Button>
      <Label fx:id="msgLbl" alignment="TOP_CENTER" layoutX="30.0" layoutY="28.0" prefHeight="98.0" prefWidth="299.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="queLbl" layoutX="51.0" layoutY="137.0" prefHeight="38.0" prefWidth="258.0" textAlignment="CENTER" textFill="RED" wrapText="true">
